<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".YearFragment">

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="407dp"
            android:layout_height="67dp"
            android:background="@color/white"
            android:orientation="horizontal"
            android:translationX="-10dp">


            <ImageButton
                android:id="@+id/previousYear"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="previousYear"
                app:srcCompat="@drawable/lasyear"
                tools:ignore="SpeakableTextPresentCheck" />

            <Button
                android:id="@+id/staticYear"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:backgroundTint="@color/cashy_grey"
                android:clickable="false"
                android:text="0"
                android:textColor="@color/black"
                android:textSize="20sp"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/nextYear"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="nextYear"
                app:srcCompat="@drawable/nextyear"
                tools:ignore="SpeakableTextPresentCheck" />

        </LinearLayout>

        <FrameLayout
